{
  "name": "@emahnovets/eslint-config-typescript",
  "version": "1.1.1",
  "description": "Base ESlint config for TS projects",
  "main": ".eslintrc.js",
  "scripts": {
    "install:pre-commit-hooks": "pre-commit install --install-hooks --allow-missing-config -t pre-commit -t prepare-commit-msg -t commit-msg -f",
    "lint": "eslint --config .eslintrc.local.js .",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emahnovets/eslint-config-typescript.git"
  },
  "keywords": [
    "eslint",
    "typescript",
    "airbnb-base"
  ],
  "author": "Evgeny Mahnovets <mail@emahnovets.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/emahnovets/eslint-config-typescript/issues"
  },
  "homepage": "https://github.com/emahnovets/eslint-config-typescript#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "eslint": "^8.24.0",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
